    "analyze": "webpack-bundle-analyzer bundle/stats.json",
    "prepublishOnly": "yarn build"
  },
  "author": "Brykou",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brykou/npm-package-webpack.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "react": "^16.13.1",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.13.1"
